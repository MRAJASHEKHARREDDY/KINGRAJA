    "def reverse(str):\n",
    "    string = \" \"\n",
    "    for i in str:\n",
    "        string = i + string\n",
    "    return string\n",
    "str = \"1234abcd\"\n",
    "print(\"The original string is:\",str)\n",
    "print(\"The reverse string is:\",reverse(str))"
   ]
